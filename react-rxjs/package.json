{
  "name": "react-rxjs",
  "version": "1.0.0",
  "description": "Demo rxjs",
  "main": "src/index.js",
  "author": "Gilbert Sewovoe-Ekoue",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel dev lint:watch test:watch",
    "dev": "webpack-dev-server -d --env.platform=browser --env.environment=development --progress --color --open",
    "precommit": "lint-staged",
    "prettier": "prettier --write ./src/**/*.js ./src/js/**/*.js",
    "lint": "eslint . && yarn run prettier",
    "lint:watch": "node_modules/eslint-watch/bin/esw -w src test webpack.config.*",
    "lint:fix": "yarn run lint -- --fix",
    "test": "mocha",
    "test:watch": "yarn test -- --watch",
    "build": "webpack -p --env.environment=production --env.platform=browser --progress"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-logger": "^1.1.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "4.3.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.3.1",
    "jsdom": "^11.1.0",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.8",
    "prettier": "^1.5.3",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.1",
    "react-test-utils": "^0.0.1",
    "redux-mock-store": "^1.2.3",
    "sinon": "^2.4.1",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
}
